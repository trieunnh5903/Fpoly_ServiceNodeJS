                        <div class="card-style mb-30">
                            <h6 class="mb-10">Data Table</h6>
                            <p class="text-sm mb-20">
                                For basic styling—light padding and only horizontal
                                dividers—use the class table.
                            </p>
                            <div class="table-wrapper table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>
                                                <h6>#</h6>
                                            </th>
                                            <th>
                                                <h6>Name</h6>
                                            </th>
                                            <th>
                                                <h6>Price</h6>
                                            </th>
                                            <th>
                                                <h6>Quantity</h6>
                                            </th>
                                            <th>
                                                <h6>Status</h6>
                                            </th>
                                            <th>
                                                <h6>Action</h6>
                                            </th>
                                        </tr>
                                        <!-- end table row-->
                                    </thead>
                                    <tbody>
                                        {{#each products}}
                                        <tr>
                                            <td>
                                                <div style="width: 150px;">
                                                    <img style="width: 150px;" src={{this.image}} alt="" />
                                                </div>
                                            </td>
                                            <td class="min-width">
                                                <p>{{this.name}}</p>
                                            </td>
                                            <td class="min-width">
                                                <p><a href="#0">{{this.price}}</a></p>
                                            </td>
                                            <td class="min-width">
                                                <p>{{this.quantity}}</p>
                                            </td>
                                            <td class="min-width" id="status">
                                                <span class="status-btn active-btn">Active</span>
                                            </td>
                                            <td>
                                                {{!-- confirmDelete('{{this._id}}'); --}}
                                                <div class="action">
                                                    <button onclick=confirmDelete('{{this._id}}') class="text-danger">
                                                        <i class="lni lni-trash-can"></i>
                                                    </button>
                                                </div>
                                            </td>
                                        </tr>
                                        {{/each}}
                                        <!-- end table row -->
                                    </tbody>
                                </table>
                                <!-- end table -->
